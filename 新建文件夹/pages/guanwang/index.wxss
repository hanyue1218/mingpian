/* pages/guanwang/index.wxss */
page{
  background: #efeff7;
}
.shouTu{
  width: 100%;
  height: 100%;
}
.shouTu image{
  width: 100%;
}
.content1Title{
  width: 100%;
  height: 100rpx;
}
.content1Title text{
  font-size: 35rpx;
  color: #8c8a8c;
  padding-left: 30rpx;
  line-height: 100rpx;
  font-family: '宋体';
  font-weight: bold;
}
.content1{
  width: 100%;
  height: 300rpx;
  background: #fff;
}
.content1-1{
  width: 690rpx;
  margin-left: 30rpx;
  font-size: 30rpx;
  font-family: '宋体';
  line-height: 50rpx;
  padding-top: 20rpx;
}
.contentNews{
  width: 100%;
  background: #fff;
  border-top: 1rpx solid #e7e7e7;
}
.newsImg{
  width: 220rpx;
  height: 150rpx;
  display: flex;
  align-items: center;
  float: left;
  margin-top: 25rpx;
  margin-left: 30rpx;
}
.newsImg image{
  width: 220rpx;
}
.news{
  width: 100%;
  height: 200rpx;
  border-bottom: 1rpx solid #e7e7e7;
}
.newsTitle{
  width: 400rpx;
  height: 100rpx;
  float: left;
  overflow: hidden;
  margin-top: 25rpx;
  margin-left: 20rpx;
}
.newsTitle text{
  display: -webkit-box;
  word-break: break-all;
  text-overflow: ellipsis;
  font-size: 30rpx;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp:2;
}
.newsTime{
  width: 300rpx;
  margin-left: 250rpx;
  font-size: 28rpx;
  color: #999;
}
.newsTime text{
  margin-left: 20rpx;
}
.teamWaikuang{
  width: 100%;
  background: #fff;
}
.team{
  width: 100%;
  /* height: 700rpx; */
  background: #fff;
}
.team1{
  width: 345rpx;
  height: 550rpx;
  margin-left: 20rpx;
  display: inline-block;
  margin-top: 30rpx;
}
.team1 image{
  width: 345rpx;
}
.teamName{
  width: 200rpx;
  border-bottom: 1rpx solid #e7e7e7;
  padding-bottom: 17rpx;
  font-size: 35rpx;
  margin-left: 30rpx;
  font-weight: 600;
  margin-top: 20rpx;
}
.zhiwei{
  font-size: 30rpx;
  font-family: '宋体';
  margin-left: 30rpx;
  margin-top: 15rpx;
}
.contact{
  width: 100%;
  height: 150rpx;
  background: #fff;
}
.contact1{
  display: inline-block;
  width: 710rpx;
  height: 90rpx;
  margin-top: 30rpx;
  margin-left: 20rpx;
  border: 1rpx solid #e7e7e7;
  font-size: 45rpx;
  text-align: center;
  line-height: 90rpx;
  color: #18ae18;
}
.map{
  width: 100%;
  height: 70rpx;
  background: #fff;
  display: flex;
  align-items: center;
}
.map image{
  float: left;
  width: 40rpx;
  margin-left: 30rpx;
}
.map text{
  font-size: 30rpx;
  color: #999;
  margin-left: 20rpx;
}
.jishu{
  width: 100%;
  height: 100rpx;
  font-size: 25rpx;
  color: #999;
  font-family: '宋体';
  line-height: 100rpx;
  text-align: center;
}